local TextChatService = game:GetService("TextChatService")
local channel = TextChatService:FindFirstChild("TextChannels")
    and TextChatService.TextChannels:FindFirstChild("RBXGeneral")
if not channel then
    channel = Instance.new("TextChannel")
    channel.Name = "RBXGeneral"
    channel.Parent = TextChatService.TextChannels
end
local msg = script:WaitForChild("Exec").Value
channel:DisplaySystemMessage(msg)
