})

addcmd({
    Name = "chatbar",
    Aliases = {},
	Player = false,
    Function = function(sender, targets, arguments)
		local option = table.concat(arguments, " ", 1)

		if option ~= "true" and option ~= "false" then
			notfi(sender, "Vecxo's admin", "The values are true or false! example: ".. prefix .."chatbar false")
