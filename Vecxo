						camera.CameraSubject = plane

						repeat task.wait() until not workspace:FindFirstChild("Airplane")

						camera.CameraSubject = game.Players.LocalPlayer.Character

						script:Destroy()

					]]

					if v.Character then
						scr.Parent = v.Character
