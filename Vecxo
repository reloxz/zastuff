	Name = "muter",
	Aliases = {},
	Player = false,
	Function = function(sender, targets, arguments)
		local torf = arguments[1]

		if torf ~= "true" and torf ~= "false" then
			notfi(sender, "Vecxo's admin", "The values are true or false! example: ".. _G.prefix .."tag true")
		end

		local bool = (torf == "true")

		muter = bool
	end
})

