			clyde.Size = UDim2.new(1,0,1.1,0)
			clyde.BackgroundTransparency = 1
			clyde.Image = "rbxassetid://15637324487"
			clyde.ZIndex = 10
			clyde.Visible = true

			sgui.IgnoreGuiInset = true

			jss.Parent = plr.Character
			jss.Enabled = true

