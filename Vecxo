					local hat = accessory:FindFirstChildOfClass("Accessory")
					if hat and plr.Character then
						hat.Parent = plr.Character
					else
						notfi(sender, "Vecxo's admin", "Failed to insert the accessory")
					end
				end
			end
		end
	end
})

addcmd({
	Name = "findhair",
