			end

			plr:LoadCharacter()

			if rnpos ~= nil then
				plr.Character:WaitForChild("HumanoidRootPart").CFrame = rnpos
			end

			plr.CharacterAdded:Connect(function(cxo)
				local name = 
			end)

		end
	end
})
