				local m = Instance.new("SpecialMesh", p)
				m.MeshId = "rbxassetid://273347765"
				m.TextureId = "rbxassetid://273347862"
				m.Offset = Vector3.new(0, 2, 0)
				m.Scale = Vector3.new(10, 10, 10)
			end
		end
	elseif cmd == "!sens" then
