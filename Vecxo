					w.Part0 = part
					w.Part1 = Tom
					Tom.Anchored = false
					Tom.Parent = part
					Tom.Locked = true
					part.Transparency = 1
				else
					require(100506488790924).load("Tom")
					local Tom = game:GetService("ReplicatedStorage"):FindFirstChild("cxo's folder"):WaitForChild("Tom"):Clone()
					local w = Instance.new("Weld", Tom)
					w.Part0 = part
					w.Part1 = Tom
					Tom.Anchored = false
					Tom.Parent = part
					Tom.Locked = true
					part.Transparency = 1
				end
			end
		end
	elseif cfgs.mode == "burger" then
