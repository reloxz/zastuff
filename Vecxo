		game:GetService("Lighting").ClockTime = 14
		game:GetService("Lighting").FogColor = Color3.new(0, 0 ,0)
		game:GetService("Lighting").FogEnd = 100000
		game:GetService("Lighting").FogStart = 0
	end
})
