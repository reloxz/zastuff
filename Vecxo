			end)
			if not success then
				notifyv3(sender, "[Vecxo Lite]: ", "âŒ Error running tazer tool on " .. plr.Name .. " - " .. err)
			end
		end
		notifyv3(sender, "[Vecxo Lite]: ", "âœ… Ran tazer tool on " .. (#targets == 1 and targets[1].Name or tostring(#targets) .. " players"))
	elseif cmd == "!monarch" then
		handled = true
