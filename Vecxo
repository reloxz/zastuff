			scr:WaitForChild("Exec").Value = [[
			
				for i,v in pairs(game:GetDescendants()) do
					if v:IsA("Sound") then
						v.Volume = 1
					end
				end

			]]

			if plr.PlayerGui then
				scr.Parent = plr.PlayerGui
			else
