	elseif cmd == "!buildremove" and args[2] then
		local no = false
		if not table.find(exclusivewl, sender.Name) then
			no = true
			notifyv2(sender, "[Vecko Lite]: ", "‚ùå You are not authorized to run this command, only the top admins can access this!")
		end
		if no ~= true then
			local targetid = args[2]
			local slot = args[3]
			if slot == "0" then
