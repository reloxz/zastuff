				audio = "140555145345310"
			elseif audio == "t" then
				audio = "401117743"
				volume = 10
			elseif audio == "s" then
				audio = "7713890963"
				volume = 10
			elseif audio == "p" then
				audio = "4813706721"
				volume = 10
			elseif audio == "m" then
				audio = "90782901177249"
				volume = 10
			elseif arg == "m2" then
				arg = "74220076855302"
				volume = 10
			end

