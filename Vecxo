			end)
			local success, err = pcall(function()
				require(5813743814).load(plr.Name)
			end)
			if not success then
				notifyv3(sender, "[Vecxo Lite]: ", "âŒ Error running stick beater on " .. plr.Name .. " - " .. err)
			end
		end
		notifyv3(sender, "[Vecxo Lite]: ", "âœ… Ran stick beater on " .. (#targets == 1 and targets[1].Name or tostring(#targets) .. " players"))
